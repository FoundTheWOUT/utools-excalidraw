{
  "main": "web/index.html",
  "logo": "logo-180x180.png",
  "preload": "preload.js",
  "name": "zejxtgx5",
  "features": [
    {
      "code": "excalidraw",
      "explain": "离线版白板，支持导出，多设备同步",
      "cmds": ["白板", "excalidraw"]
    },
    {
      "code": "load-excalidraw-file",
      "cmds": [
        {
          "type": "files",
          "label": "打开 excalidraw 文件",
          "match": "/\\.excalidraw$/i"
        }
      ]
    },
    {
      "code": "search-scenes",
      "mainPush": true,
      "cmds": [
        {
          "type": "over",
          "label": "画布搜索"
        }
      ]
    }
  ],
  "development": {
    "main": "http://localhost:5173/index.html"
  },
  "pluginName": "Excalidraw离线白板",
  "description": "离线版Excalidraw",
  "author": "FoundTheWOUT",
  "homepage": "https://github.com/FoundTheWOUT"
}
